@mixin oif {
    {{# if selector }}
    {{ selector }}:before {
    {{ else }}
    {{ tag }}[class^="{{prefix}}-"]:before, {{ tag }}[class*=" {{prefix}}-"]:before {
    {{/ if }}
        font-family: {{ name }} !important;
        font-style: normal;
        font-weight: normal !important;
        font-variant: normal;
        text-transform: none;
        line-height: 1;
        font-display: swap;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
    }
}

{{# each codepoints }}
@mixin {{ ../prefix }}-{{ @key }} {
    content: "\\{{ codepoint this }}";
}
{{/ each }}
